{
  "name": "pdflow",
  "version": "0.4.0",
  "description": "Multimodal PDF-to-structured-data extraction app with CLI support",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "cli:build": "tsc src/cli/pdflow.ts --outDir dist --module commonjs --esModuleInterop --skipLibCheck --resolveJsonModule",
    "cli": "node dist/cli/pdflow.js",
    "pdflow": "tsx src/cli/pdflow.ts"
  },
  "bin": {
    "pdflow": "./dist/cli/pdflow.js"
  },
  "keywords": [
    "pdf",
    "ai",
    "extraction",
    "nextjs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "ai": "^5.0.87",
    "commander": "^14.0.2",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.1",
    "framer-motion": "^12.23.24",
    "handlebars": "^4.7.8",
    "next": "^16.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-poppler": "^0.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "shadcn-ui": "^0.9.5",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "tsx": "^4.20.6"
  }
}
